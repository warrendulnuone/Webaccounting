Obj.ajaxURL,'__FormID={grid.componentName}&__FormAction=GenerateTreeBranches&__treeType=externalnavigator&__treeName=TREENAVIGATOR&__treeObject={grid.componentName}_EXTERNALNAVIGATOR_TREENAVIGATOR&'+$gvs('{grid.componentName}.STATEINFO'));%str%>\n<tree<type=\"dynamic\">\n<levels<[1]<type=\"SQL\">\n<connection=\"::Name::SQL\">\n<statement=\"SELECT glv1 AS Column1, glv1 FROM Accountswithglv  GROUP BY glv1, glv1 ORDER BY glv1\">\n<htmlTemplate=\"{Column1}\">\n<dataTemplate=\"{baseFilter: '', userFilter: 'glv1 = \\'{glv1}\\'', link: '', order: '', params: ''}\">\n>\n<[2]<type=\"SQL\">\n<connection=\"::Name::SQL\">\n<statement=\"SELECT glv2 AS Column1, glv2 FROM Accountswithglv  WHERE glv1 = :level1 GROUP BY glv2, glv2 ORDER BY glv2\">\n<htmlTemplate=\"{Column1}\">\n<dataTemplate=\"{baseFilter: '', userFilter: 'glv2 = \\'{glv2}\\'', link: '', order: '', params: ''}\">\n>\n<[3]<type=\"SQL\">\n<connection=\"::Name::SQL\">\n<statement=\"SELECT glv3 AS Column1, glv3 FROM Accountswithglv  WHERE glv1 = :level1  AND  glv2 = :level2 GROUP BY glv3, glv3 ORDER BY glv3\">\n<htmlTemplate=\"{Column1}\">\n<dataTemplate=\"{baseFilter: '', userFilter: 'glv3=\\'{glv3}\\'', link: '', order: '', params: ''}\">\n>\n<[4]<type=\"SQL\">\n<connection=\"::Name::SQL\">\n<statement=\"SELECT glv4 AS Column1, glv4 FROM Accountswithglv  WHERE glv1 = :level1  AND  glv2 = :level2  AND  glv3 = :level3 GROUP BY glv4, glv4 ORDER BY glv4\">\n<htmlTemplate=\"{Column1}\">\n<dataTemplate=\"{baseFilter: '', userFilter: 'glv4 = \\'{glv4}\\'', link: '', order: '', params: ''}\">\n>\n<[5]<type=\"SQL\">\n<connection=\"::Name::SQL\">\n<statement=\"SELECT glv5 AS Column1, glv5 FROM Accountswithglv  WHERE glv1 = :level1  AND  glv2 = :level2  AND  glv3 = :level3  AND  glv4 = :level4 GROUP BY glv5, glv5 ORDER BY glv5\">\n<htmlTemplate=\"{Column1}\">\n<dataTemplate=\"{baseFilter: '', userFilter: 'glv5 = \\'{glv5}\\'', link: '', order: '', params: ''}\">\n>\n>\n<id=\"{grid.componentName}.EXTERNALNAVIGATOR.TREENAVIGATOR\"